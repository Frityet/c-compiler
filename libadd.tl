local ffi = require("ffi")

local cdef = [[
/* generated by autogen_ffi */
int add(int a, int b);
]]

ffi.cdef(cdef)

local record libadd
   add: ffi.Function<function(ffi.Function<nil>, a: integer, b: integer): integer>
end

local STRIP_PREFIX = ""

local libadd: libadd = ffi.load("t") as libadd

return libadd
